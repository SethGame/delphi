# LLM Provider Configuration
# Set to "azure_openai" or "gemini" to choose your LLM provider
LLM_PROVIDER=gemini

# Azure OpenAI Configuration (required if LLM_PROVIDER=azure_openai)
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret
AZURE_TENANT_ID=your_azure_tenant_id
AZURE_OPENAI_DEPLOYMENT=your_deployment_name

# Google Gemini Configuration (required if LLM_PROVIDER=gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# Chainlit Configuration
CHAINLIT_AUTH_SECRET=your_secret_key_here 